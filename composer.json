{
    "name": "jauntin/two-factor-auth",
    "description": "Laravel package for two factor authentication via email",
    "keywords": [
        "jauntin",
        "authentication",
        "two-factor-auth"
    ],
    "homepage": "https://github.com/jauntin/two-factor-auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alex Andreiev",
            "email": "alex@jauntin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1 <8.4",
        "doctrine/dbal": "^3.6",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "larastan/larastan": "^2.9.7",
        "orchestra/testbench": "^8.23.2",
        "phpunit/phpunit": "^10.5.20"
    },
    "autoload": {
        "psr-4": {
            "Jauntin\\TwoFactorAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jauntin\\TwoFactorAuth\\TwoFactorAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
