{
  "name": "jauntin/two-factor-auth",
  "description": "Laravel package for two factor authentication via email",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "jauntin",
    "authentication",
    "two-factor-auth"
  ],
  "authors": [
    {
      "name": "Alex Andreiev",
      "email": "alex@jauntin.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/jauntin/two-factor-auth",
  "require": {
    "php": ">=8.3 <8.4",
    "laravel/framework": "^10.0 || ^11.0 || ^12.0"
  },
  "require-dev": {
    "larastan/larastan": "^2.9.7 || ^3.2",
    "laravel/pint": "^1.21.2",
    "orchestra/testbench": "^8.34 || ^9 || ^10.1",
    "phpunit/phpunit": "^10.5.45 || ^11.5.14"
  },
  "autoload": {
    "psr-4": {
      "Jauntin\\TwoFactorAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jauntin\\TwoFactorAuth\\TwoFactorAuthServiceProvider"
      ]
    }
  }
}
